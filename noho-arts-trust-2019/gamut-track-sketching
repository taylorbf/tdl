d1 $ jux (rev)
   $ randcat [
        n "[12 14 15]*<3 4>" # sound "p" # cps 1 # sustain 3,
        n "[10 14 16]*<3 4>" # sound "p" # cps 1 # sustain 3
        --n "<5 4 5 7>" # s "p" # cps 5 # sustain 10,
        --n "[15, 19, 21]" # s "p" # cps 0.2 # sustain 10
     ]
   # cutoff 1000
   # gain (slow 10 $ range 0.6 1 $ sine)

d3 $ n "<1 5>/<6>"
    # s "p"
    # cutoff 500

hush
